import java.io.IOException;
import java.util.Scanner;

public class Main {
 
    public static void main(String[] args) throws IOException {
 
    Scanner teclado = new Scanner (System.in);
     float A,B,C,D;
     
     A = teclado.nextFloat();
     
     B = teclado.nextFloat();
     
     C = teclado.nextFloat();
     
     D = (A * 2 + B * 3 + C * 5)/10;
     
     System.out.printf("\nMEDIA = %.1f",+D);
    }
 
}
