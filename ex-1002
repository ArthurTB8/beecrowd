import java.io.IOException;
import java.util.Scanner;
 

public class Main {
 
    public static void main(String[] args) throws IOException {
        Scanner teclado = new Scanner (System.in);
    
    double A,B,C;    
    
    A = teclado.nextDouble();
    B = A * A;
    C = B * 3.14159;
    
    System.out.println("A= "+C);
    
 
      
    }
 
}
